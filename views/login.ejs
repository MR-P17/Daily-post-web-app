<%- include("partials/loginHeader"); -%>

<h1>Login page</h1>

<form class="" action="/home" method="post">
<div class="form-group">
  <label>Email id</label>
  <input class="form-control" type="email" name="email" placeholder="Enter your registered email" style="width:30%;"><br>
  <label>Password</label>
  <input class="form-control" type="password" name="password" placeholder="Enter your password" style="width:30%;"><br>
</div>
<button class="btn btn-primary" type="submit" name="button">Login</button>
</form>
